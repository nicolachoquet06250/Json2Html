{
  "nohtml": {
    "head": "toto"
  },
  "Doctype": "html",
  "html": {
    "head": {
      "metas": [
        {
          "name": "Description",
          "content": "Voila une jolie description"
        },
        {
          "charset": "utf-8"
        }
      ],
      "title": "ma premiere page HTML en Json",

      "links": [
        {
          "type": "text/css",
